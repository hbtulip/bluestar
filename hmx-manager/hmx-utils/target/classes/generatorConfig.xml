<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
"http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<!-- 向 classpath下 添 加 JDBC驱 动 -->
	<classPathEntry location="./lib/mysql-connector-java-5.1.18.jar" />
	
	<context id="MysqlTables_customer_cust" targetRuntime="MyBatis3">
		<plugin type="org.mybatis.generator.plugins.SerializablePlugin" />
		<commentGenerator>
			<property name="suppressAllComments" value="false" />
			<property name="suppressDate" value="true" />
		</commentGenerator>
		<!-- 元 素 定 义 如 何 连 接 目 标 数 据 库 -->
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://47.92.94.224:3306/oilunion"
			userId="root" password="su123456">
		</jdbcConnection>
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>
		<!-- 元 素 来 指 定 生 成Java模 型 对 象 所 属 的 包 -->
		<javaModelGenerator
			targetPackage="org.hmx.utils.storage.domain"
			targetProject="src/main/java">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>
		<!-- 元 素 指 定 生 成 SQL映 射 文 件 的 位 置 与 目 标 项 目 -->
		<sqlMapGenerator
			targetPackage="org.hmx.utils.storage.mapper"
			targetProject="src/main/java">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>
		<javaClientGenerator type="ANNOTATEDMAPPER"
			targetPackage="org.hmx.utils.storage.mapper"
			targetProject="src/main/java">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>


		<table tableName="t_division" domainObjectName="Division">
			<generatedKey column="id" sqlStatement="JDBC" identity="true" type="post" />
		</table>
		
<!--
		<table tableName="t_commission" domainObjectName="Commission">
			<generatedKey column="id" sqlStatement="JDBC" identity="true" type="post" />
		</table>

		
		<table tableName="t_store_article" domainObjectName="StoreArticle">
			<columnOverride column="content" javaType="String" jdbcType="VARCHAR">
			</columnOverride>
		</table>
 -->


	</context>
</generatorConfiguration>